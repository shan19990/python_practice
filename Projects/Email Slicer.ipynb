    "    username = email.split(\"@\")[0]\n",
    "    email = email.split(\"@\")[1]\n",
    "    domain = email.split(\".\")[0]\n",
    "    email = email.split(\".\")[1]\n",
    "    extention = email\n",
    "    return username,domain,extention"
