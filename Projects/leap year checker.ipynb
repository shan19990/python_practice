   "source": [
    "def leap_year(year):\n",
    "    return ((year % 4 == 0) and (year % 100 != 0)) or (year % 400 == 0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "e6c0775a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter year you want to check if leap year : 2024\n",
      "Its a leap year\n"
     ]
    }
   ],
   "source": [
    "year = int(input(\"Enter year you want to check if leap year : \"))\n",
    "if leap_year(year):\n",
    "    print(\"Its a leap year\")\n",
    "else:\n",
    "    print(\"Its not a leap year\")"
   ]
